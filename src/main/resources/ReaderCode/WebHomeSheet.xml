<content>{{velocity}}
#set($ok = $xwiki.ssx.use('ReaderCode.WebHomeSheet'))
#set($ok = $xwiki.ssx.use('Main.Dashboard'))

{{html}}
#if($hasEdit)
&lt;div id="readerLink"&gt;
&lt;a href="$xwiki.getDocument('Stream').getURL('view')"&gt;→ Access the web reader&lt;/a&gt;
&lt;/div&gt;
#end
#if($hasAdmin)
&lt;div id="preferencesLink"&gt;
&lt;a href="$xwiki.getDocument('Status').getURL()"&gt;→ Status&lt;/a&gt;
&lt;/div&gt;
#end
#* #if($hasAdmin)
&lt;div id="preferencesLink"&gt;
&lt;a href="$xwiki.getDocument('WebPreferences').getURL('admin',"editor=spaceadmin&amp;section=CWW&amp;space=$doc.space")"&gt;→ Preferences&lt;/a&gt;
&lt;/div&gt;
#end *# ## Not ready for prime time
{{/html}}

(% class="dashboard" %)
(((
  (% class="dashboardleft" %)
  (((
    (% class="dashboardleftcontent" %)
    (((
= Flagged Articles =
{{flaggedArticles limit=10 emptyListMessage="There are no flagged articles for the moment. Grab a coffee and come back later ? Or maybe start flagging some interesting articles !"/}}

= Recently Fetched =
{{readerArticles limit=3 emptyListMessage="There are no fetched article for the moment. If you have already added sources to your reader, some articles should start being fetched very soon."/}}

= $msg.get("xe.dashboard.wiki.tagcloud") =
#set ($tagCloudSpace = $doc.space)

{{tagcloud  space="$tagCloudSpace" /}}

= Sources =
{{include document="ReaderCode.Sources" emptyListMessage="No flagged articles for the moment. Come back later ?"/}}
    )))
  )))
  (% class="dashboardright" %)
  (((
    (% class="dashboardrightcontent" %)
    (((

= Articles reviews =
{{reviews className="XWiki.FeedEntryClass" /}}

= $msg.get("xe.dashboard.space.activity", [$doc.space]) =
## Set variables to limit recent activity to the current space and 15 items.
#set ($aSpace = $doc.space)

{{activity spaces="$!aSpace" /}}

    )))
  )))
)))
(% class="clearfloats" %)((( )))

{{/velocity}}</content></xwikidoc>
